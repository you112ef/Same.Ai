{
  "dashboard": {
    "id": null,
    "title": "AI Coding Assistant Dashboard",
    "tags": ["ai", "coding", "assistant", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{method}} {{route}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "yAxes": [
          {
            "label": "Requests per second",
            "unit": "reqps"
          }
        ]
      },
      {
        "id": 2,
        "title": "Response Time",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_request_duration_seconds_sum[5m]) / rate(http_request_duration_seconds_count[5m])",
            "legendFormat": "{{method}} {{route}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "yAxes": [
          {
            "label": "Response time",
            "unit": "s"
          }
        ]
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{status=~\"5..\"}[5m]) / rate(http_requests_total[5m]) * 100",
            "legendFormat": "Error Rate"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "yAxes": [
          {
            "label": "Error rate",
            "unit": "percent"
          }
        ]
      },
      {
        "id": 4,
        "title": "Active Sessions",
        "type": "stat",
        "targets": [
          {
            "expr": "ai_coding_assistant_active_sessions",
            "legendFormat": "Active Sessions"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 8
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 50
                },
                {
                  "color": "red",
                  "value": 100
                }
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "nodejs_heap_size_used_bytes / nodejs_heap_size_total_bytes * 100",
            "legendFormat": "Heap Usage"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        },
        "yAxes": [
          {
            "label": "Memory usage",
            "unit": "percent"
          }
        ]
      },
      {
        "id": 6,
        "title": "Redis Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "redis_memory_used_bytes / redis_memory_max_bytes * 100",
            "legendFormat": "Redis Memory"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        },
        "yAxes": [
          {
            "label": "Memory usage",
            "unit": "percent"
          }
        ]
      },
      {
        "id": 7,
        "title": "AI API Calls",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(ai_api_calls_total[5m])",
            "legendFormat": "API Calls"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        },
        "yAxes": [
          {
            "label": "Calls per second",
            "unit": "calls"
          }
        ]
      },
      {
        "id": 8,
        "title": "File Operations",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(file_operations_total[5m])",
            "legendFormat": "{{operation}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 24
        },
        "yAxes": [
          {
            "label": "Operations per second",
            "unit": "ops"
          }
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ]
    },
    "templating": {
      "list": [
        {
          "name": "service",
          "type": "query",
          "query": "label_values(ai_coding_assistant_requests_total, service)",
          "refresh": 2
        },
        {
          "name": "route",
          "type": "query",
          "query": "label_values(ai_coding_assistant_requests_total, route)",
          "refresh": 2
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "expr": "changes(ai_coding_assistant_version[1m]) > 0",
          "iconColor": "red",
          "text": "New deployment detected"
        }
      ]
    },
    "refresh": "30s",
    "schemaVersion": 16,
    "version": 1
  }
}