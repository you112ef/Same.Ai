{
  "name": "ai-coding-assistant",
  "version": "1.0.0",
  "description": "AI Coding Assistant - Your intelligent coding companion",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "build": "echo 'No build step required for Node.js server'",
    "build:frontend": "cd frontend && npm install && npm run build",
    "build:all": "npm run build:frontend",
    "install:all": "npm install && cd frontend && npm install",
    "dev:all": "concurrently \"npm run dev\" \"cd frontend && npm start\"",
    "test": "echo 'No tests specified'"
  },
  "dependencies": {
    "archiver": "^6.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "fs-extra": "^11.1.1",
    "helmet": "^7.1.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.20.1",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@typescript-eslint/parser": "^8.41.0",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "ai",
    "coding",
    "assistant",
    "openai",
    "gpt",
    "development",
    "programming"
  ],
  "author": "AI Coding Assistant Team",
  "license": "MIT"
}
