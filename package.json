{
  "name": "ai-coding-assistant",
  "version": "1.0.0",
  "description": "AI-powered web development assistant with live preview and no registration required",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "vite client",
    "build": "vite build client",
    "start": "node server/index.js",
    "docker:build": "docker build -t ai-coding-assistant .",
    "docker:run": "docker run -p 3000:3000 -p 5173:5173 ai-coding-assistant"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "dotenv": "^16.3.1",
    "openai": "^4.20.1",
    "dockerode": "^3.3.5",
    "fs-extra": "^11.1.1",
    "chokidar": "^3.5.3",
    "glob": "^10.3.10",
    "simple-git": "^3.20.0",
    "archiver": "^6.0.1",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.1",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0",
    "compression": "^1.7.4",
            "express-rate-limit": "^7.1.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0",
    "vite": "^4.4.9",
    "@vitejs/plugin-react": "^4.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.2",
    "@types/node": "^20.5.7",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/multer": "^1.4.7",
    "@types/uuid": "^9.0.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^9.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "ai",
    "coding",
    "assistant",
    "web-development",
    "live-preview",
    "no-registration"
  ],
  "author": "AI Coding Assistant Team",
  "license": "MIT"
}