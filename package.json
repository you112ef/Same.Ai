{
  "name": "ai-coding-assistant",
  "version": "1.0.0",
  "description": "AI-powered web development assistant with live preview and no registration required",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "build:all": "npm run build:frontend",
    "install:all": "npm install && cd frontend && npm install",
    "dev:all": "concurrently \"npm run server\" \"cd frontend && npm start\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "eslint . --ext .js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write \"**/*.{js,ts,jsx,tsx,json,md}\"",
    "docker:build": "docker build -t ai-coding-assistant .",
    "docker:run": "docker run -p 3000:3000 -p 3001:3001 ai-coding-assistant",
    "docker:compose": "docker-compose up --build",
    "docker:compose:down": "docker-compose down",
    "clean": "rm -rf node_modules package-lock.json && npm install",
    "clean:all": "rm -rf node_modules package-lock.json frontend/node_modules frontend/package-lock.json && npm run install:all",
    "logs": "tail -f logs/app.log",
    "health": "curl http://localhost:3000/api/health"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "dotenv": "^16.3.1",
    "openai": "^4.20.1",
    "fs-extra": "^11.1.1",
    "archiver": "^6.0.1",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "body-parser": "^1.20.2",
    "morgan": "^1.10.0",
    "express-validator": "^7.0.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cookie-parser": "^1.4.6",
    "express-session": "^1.17.3",
    "connect-redis": "^7.1.0",
    "redis": "^4.6.10",
    "pg": "^8.11.3",
    "sequelize": "^6.35.0",
    "nodemailer": "^6.9.7",
    "sharp": "^0.32.6",
    "imagemin": "^8.0.1",
    "chokidar": "^3.5.3",
    "glob": "^10.3.10",
    "minimatch": "^9.0.3",
    "commander": "^11.1.0",
    "ora": "^7.0.1",
    "chalk": "^5.3.0",
    "inquirer": "^9.2.12",
    "figlet": "^1.6.0",
    "boxen": "^7.1.1",
    "gradient-string": "^2.0.2",
    "terminal-link": "^3.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-node": "^11.1.0",
